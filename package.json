    "axios": "0.19.2",
    "history": "4.10.1",
    "leaflet": "1.7.1",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.2.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
